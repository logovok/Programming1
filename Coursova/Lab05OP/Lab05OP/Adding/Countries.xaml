        xmlns:local="clr-namespace:Lab05OP.Adding"
        mc:Ignorable="d"
        Title="Countries" Height="227" Width="326">
    <Grid Margin="0,0,-47,0">
        <TextBox x:Name="TB1" HorizontalAlignment="Left" Height="22" Margin="35,97,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="146" FontFamily="Bahnschrift Condensed"/>
