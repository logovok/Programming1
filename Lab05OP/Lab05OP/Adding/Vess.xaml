    <Grid Margin="0,0,10,-6">
        <TextBox x:Name="TB1" HorizontalAlignment="Left" Height="22" Margin="35,97,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="146" FontFamily="Bahnschrift Condensed"/>
        <TextBlock HorizontalAlignment="Left" Height="21" Margin="181,95,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="132" FontFamily="Bahnschrift Condensed"/>
        <Button Content="Add!" HorizontalAlignment="Left" Height="24" Margin="35,242,0,0" VerticalAlignment="Top" Width="212" FontFamily="Bahnschrift Condensed" FontSize="22" Click="Button_Click"/>
        <TextBlock HorizontalAlignment="Left" Height="55" Margin="10,10,0,0" TextWrapping="Wrap" Text="Add new vessel" VerticalAlignment="Top" Width="246" FontSize="24" FontFamily="Bahnschrift Condensed"/>
        <TextBox x:Name="TB2" HorizontalAlignment="Left" Height="22" Margin="35,125,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="146" FontFamily="Bahnschrift Condensed"/>
