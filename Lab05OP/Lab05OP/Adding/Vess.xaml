        <ComboBox x:Name="CB2" HorizontalAlignment="Left" Height="24" Margin="35,212,0,0" VerticalAlignment="Top" Width="146"/>
        <TextBlock HorizontalAlignment="Left" Height="20" Margin="181,216,0,0" TextWrapping="Wrap" Text="Country of registration" VerticalAlignment="Top" Width="132" FontFamily="Bahnschrift Condensed"/>

